{
  "env": {
    "node": true,
    "commonjs": true,
    "es2021": true,
    "jest/globals": true
  },
  "extends": ["eslint:recommended", "prettier"],
  "plugins": ["jest"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "globals": {
    "fetch": false
  },
  "rules": {
    "strict": ["error", "never"],
    "no-unused-vars": [2, { "args": "all", "argsIgnorePattern": "^_" }]
  }
}
